{
  "base_keywords": [
    "MacBook Pro",
    "MacBook Air"
  ],
  "headless": true,
  "use_tor": true,
  "tor_socks_port": 9150,
  "selenium_wait_timeout": 15,
  "sites": {
    "ebay_us": {
      "base_url": "https://www.ebay.com",
      "search_bar_selector": "input#gh-ac",
      "autocomplete_container_selector": "ul#ebay-autocomplete",
      "autocomplete_parser_type": "ebay_list",
      "search_url_template_with_category": "https://www.ebay.com/sch/i.html?_from=R40&_nkw={search_term}&_sacat={category_id}&LH_TitleDesc=0&rt=1&LH_PrefLoc=1&_ipg=240",
      "search_url_template_no_category": "https://www.ebay.com/sch/i.html?_from=R40&_nkw={search_term}&_sacat=0&LH_TitleDesc=0&rt=1&LH_PrefLoc=1&_ipg=240",
      "allowed_category_keywords": ["laptop", "netbook", "laptops & netbooks", "apple laptops"],
      "allow_search_without_category_if_suggestion_had_no_category": true
    }
  }
}